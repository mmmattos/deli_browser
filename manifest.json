{
  "name": "Online Bookmarks Browser Extension",
  "manifest_version": 2,
  "version": "2.0",
  "description": "This extension allows for browsing any delicious account with public bookmarks.",
  "icons": {  "16": "img/deli_browser_icon16.png",
              "48": "img/deli_browser_icon48.png",
             "128": "img/deli_browser_icon128.png" },
  "browser_action": {
    "default_icon": "img/deli_browser_icon48.png",
    "default_name": "Online Bookmarks Browser Extension",
    "default_popup": "deli_browser.html"
  },
  "content_security_policy": "script-src 'self' https://feeds.delicious.com; object-src 'self'",
  "options_page": "settings.html",
  "permissions": [ 
    "tabs",
    "contextMenus",
    "storage",
    "webRequest",
    "*://delicious.com/*",
    "*://*.delicious.com/*" ]
}
